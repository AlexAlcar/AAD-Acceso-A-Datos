<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida.examen2evaad">
	<class name="Precio" table="precios">
		<id name="id" column="id">
			<generator class="increment"/>
		</id>
		<property name="item" type="string" column="item"/>
		<property name="precio" type="int" column="precio"/>
		<property name="precioOferta" type="int" column="precioOferta"/>
		<property name="saldo" type="string" column="saldo"/>
	</class>
</hibernate-mapping>